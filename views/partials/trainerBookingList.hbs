<h1>Bookings</h1>
<section class="ui raised segment">
        <table class="inverted blue ui six column table">
            <thead>
            <tr>
                <th>Date</th>
                <th>Time</th>
                <th>Member</th>
                <th>Update Booking</th>
                <th>Delete Booking</th>
                <th>Add Assessment</th>
            </tr>
            </thead>
            <tbody>
            {{#each trainer.bookings}}
                <tr>
                    <td> {{date}} </td>
                    <td> {{time}} </td>
                    <td> {{mfirstName}} {{mlastName}} </td>
                    <td> <a href="/bookings/{{userId}}/trainerUpdateBooking/{{bookingId}}" class="ui icon button"><i class="edit icon"></i></a></td>
                    <td> <a href="/bookings/{{userId}}/trainerDeleteBooking/{{bookingId}}" class="ui icon button"><i class="trash icon"></i></a></td>
                    <td> <a href="" class="ui icon button"><i class="file text outline icon"></i></a></td>
                </tr>
            {{/each}}
            </tbody>
        </table>
</section>