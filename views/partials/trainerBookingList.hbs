<h1>Bookings</h1>
<section class="ui raised segment">
    {{#each trainer.bookings}}
        <table class="inverted blue ui five column table">
            <thead>
            <tr>
                <th>Date</th>
                <th>Time</th>
                <th>Member</th>
                <th></th>
                <th></th>
            </tr>
            </thead>
            <tbody>
            <tr>
                <td> {{date}} </td>
                <td> {{time}} </td>
                <td> {{mfirstName}} {{mlastName}} </td>
                <td> <a href="" class="ui icon button"><i class="edit icon"></i></a></td>
                <td> <a href="" class="ui icon button"><i class="trash icon"></i></a></td>
            </tr>
            </tbody>
        </table>
    {{/each}}
</section>